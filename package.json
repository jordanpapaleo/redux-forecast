{
  "name": "redux-seed",
  "version": "1.0.0",
  "description": "A web seed project using webpack, react, es6, redux, authentication",
  "keywords": [
    "react",
    "redux",
    "seed",
    "es6"
  ],
  "main": "./app/index.js",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --progress --inline --hot",
    "test": "npm run lint-standard",
    "build-dist": "webpack --config webpack.config-dist.js --progress --bail -p",
    "dist": "npm test && rm -rf dist && npm run build-dist && cp public/images/apple-icons/* dist",
    "lint-standard": "standard *.js, ./app/*.js, ./app/**/*.js | standard-reporter --stylish",
    "lint-test": "npm run lint-standard --force"
  },
  "license": "ISC",
  "author": "Jordan Papaleo",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "dist"
    ]
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "bootstrap": "git://github.com/twbs/bootstrap.git#v4-dev",
    "classnames": "2.2.3",
    "gsap": "1.18.2",
    "lodash": "^4.3.0",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-google-maps": "^4.7.1",
    "react-icons": "^1.0.3",
    "react-sparklines": "^1.4.2",
    "redux-promise": "^0.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "6.3.26",
    "babel-eslint": "^4.1.8",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "0.23.1",
    "debug": "2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.8.1",
    "less": "2.5.3",
    "less-loader": "2.2.2",
    "node-sass": "^3.4.2",
    "sass-loader": "3.1.2",
    "standard": "^6.0.5",
    "standard-reporter": "^1.0.5",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  }
}
